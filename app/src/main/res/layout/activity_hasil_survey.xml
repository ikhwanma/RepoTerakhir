<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HasilSurvey">

    <View
        android:id="@+id/view2"
        android:layout_width="295dp"
        android:layout_height="261dp"
        android:layout_marginTop="251dp"
        android:background="@drawable/hasil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDaftar"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_daftar"
        android:fontFamily="@font/montserrat"
        android:text="Daftar Rumah Sakit Rujukan COVID - 19"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <Button
        android:id="@+id/btnKembali"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_marginTop="20dp"
        android:text="Kembali Ke Beranda"
        android:textSize="12dp"
        android:textStyle="bold"
        android:fontFamily="@font/montserrat"
        android:background="@drawable/btn_kembali"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDaftar" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="44dp"
        android:src="@drawable/garis_lurus"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/view2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="287dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/montserrat"
        android:text="Apabila anda mengalami gejala atau kondisi terbaru terhadap penyakit anda, anda dapat melakukan check kembali."
        android:textAlignment="center"
        android:textColor="#21243C"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2" />

    <TextView
        android:id="@+id/textHasil"
        android:layout_width="285dp"
        android:layout_height="140dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textColor="#21243C"
        android:textAlignment="center"
        android:textSize="14dp"
        android:fontFamily="@font/montserrat"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />
</androidx.constraintlayout.widget.ConstraintLayout>
